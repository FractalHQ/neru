{
    "name": "neru",
    "version": "0.3.0",
    "description": "Neru is the file router for express.",
    "author": "GHOST",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "dev": "run-p dev:*",
        "dev:ts": "tsup-node --watch src",
        "dev:nodemon": "nodemon dev/src/index.js",
        "build": "tsup-node"
    },
    "devDependencies": {
        "@types/node": "^16.11.4",
        "discord.js": "^13.0.0",
        "nodemon": "^2.0.14",
        "npm-run-all": "^4.1.5",
        "tsup": "^5.5.0",
        "typescript": "^4.4.4"
    },
    "homepage": "https://github.com/NeruJS/neru",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NeruJS/neru"
    },
    "bugs": {
        "url": "https://github.com/NeruJS/neru"
    },
    "engines": {
        "node": ">=12"
    },
    "dependencies": {
        "consolite": "^0.2.2",
        "joi": "^17.4.2"
    }
}
